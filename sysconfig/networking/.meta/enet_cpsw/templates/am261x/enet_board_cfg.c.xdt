#define ENET_GET_NUM_MAC_ADDR(num)                                        ((num>>3)+1)
#define ENET_MAC_ADDR_VALIDATE_MASK                                       (0x01U)

#define I2C_ADDRESS_IO_EXPANDER_TCA6408                                   (0x21U)

